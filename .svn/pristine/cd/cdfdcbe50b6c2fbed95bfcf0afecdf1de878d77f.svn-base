body {
	margin: 0;
	padding: 0;
	overflow-x: hidden;
}

html {
	height: 100%;
}

img {
	border: none;
}

.container {
	width: 100%;
	height: 630px;
	margin: auto;
	background-color: #eeeeee;
}

.topbar {
	background-color: #3992d0;
	width: 100%;
	margin: 0;
	height: 80px;
}

.top {
	width: 100%;
	margin: 0 auto;
	font-size: 30px;
	text-align: left;
	padding-top: 20px;
	padding-left: 50px;
	color: white;
}

.leftsidebar_box {
	width: 160px;
	height: auto !important;
	overflow: visible !important;
	position: fixed;
	height: 100% !important;
	background-color: #273542;
	height: 600px;
	float: left;
	display: inline;
}

.content {
	margin: 0px;
	width: 800px;
	font-size: 20px;
	display: inline;
}

caption {
	font-size: 25px;
}

table {
	border: 2px solid #ff0000;
	border-collapse: collapse;
	width: 1000px;
	margin: 20px auto auto 170px;
}

thead {
	background-color: #008080;
	color: white;
}

thead tr th {
	height: 50px;
	border: 2px solid white;
}

tbody tr:nth-child(odd) {
	background-color: white;
}

tbody tr:nth-child(even) {
	background-color: beige;
}

td {
	text-align: center;
	height: 40px;
	border: 2px solid cadetblue;
}

table tbody tr td table {
	margin: 0 0 0 0;
	width: 100px;
}

.RadioStyle {
	margin: 0px auto auto 160px;
	font-size: 20px;
	width: 400px;
	left: 160px;
	float: left;
}

input[type="submit"] {
	margin: 0px auto auto 800px;
	background: url('/user/images/buttom.png') no-repeat;
	font-size: 15px;
	font-weight: 600;
	color: #FFFFFF;
	border: 0;
	font-family: "Microsoft YaHei", "SimHei", "SimSun";
	text-shadow: 0 -1px 0 #535353;
	padding: 0 0 3px 0;
	display: block;
	width: 106px;
	height: 35px;
	text-align: center;
	font-weight: bold;
	line-height: 33px;
	text-indent: 20px;
}

.role {
	margin: 20px auto auto 70px;
}

.note {
	margin: 0px auto auto;
	color: #232323;
}

.auth {
	margin: 20px auto auto 70px;
}

.auth span {
	color: red;
}

.RadioStyle label {
	border: 1px solid #008080;
	color: #000000;
	padding: 2px 10px 2px 5px;
	line-height: 28px;
	min-width: 80px;
	text-align: center;
	margin: 2px;
	border-radius: 4px
}

.RadioStyle input:checked+label {
	background: url(/user/images/ico_checkon.svg) no-repeat right bottom;
	border: 1px solid #00a4ff;
	background-size: 21px 21px;
	color: #00a4ff
}

.RadioStyle input:disabled+label {
	opacity: 0.7;
}