img {
	border: none;
}

dl, dt, dd {
	display: block;
	margin: 0;
}

a {
	text-decoration: none;
}

#bg {
	background-image: url(../user/images/content/dotted.png);
}

/*left*/
.leftsidebar_box {
	width: 160px;
	height: auto !important;
	overflow: visible !important;
	position: fixed;
	height: 100% !important;
	background-color: #273542;
}

.leftsidebar_box
.line {
	height: 2px;
	width: 100%;
	background-image: url(/user/images/left/line_bg.png);
	background-repeat: repeat-x;
}

.leftsidebar_box dt {
	padding-left: 50px;
	padding-right: 10px;
	background-repeat: no-repeat;
	background-position: 10px center;
	color: #f5f5f5;
	font-size: 14px;
	position: relative;
	line-height: 48px;
	cursor: pointer;
}

.leftsidebar_box dt img {
	position: absolute;
	right: 10px;
	top: 20px;
}

.custom dt {
	background-image: url(/user/images/left/custom.png)
}

.custom dt a {
	color: white;
	font-size: 20px;
}

.syetem_management dt {
	background-image: url(/user/images/left/syetem_management.png)
}

.syetem_management dt a {
	color: white;
	font-size: 20px;
}